# Database Configuration
POSTGRES_DB=photohelper_db
POSTGRES_USER=photohelper_user
POSTGRES_PASSWORD=your_secure_password_here

# Database Connection URL (for application)
DATABASE_URL=postgresql://photohelper_user:your_secure_password_here@postgres:5432/photohelper_db

# Redis Configuration
REDIS_URL=redis://redis:6379/0

# Celery Configuration
CELERY_BROKER_URL=redis://redis:6379/0
CELERY_RESULT_BACKEND=redis://redis:6379/0

# Application Configuration
APP_NAME=PhotoHelper
APP_ENV=development
DEBUG=True
SECRET_KEY=your_secret_key_here_change_in_production

# API Configuration
API_V1_PREFIX=/api/v1
CORS_ORIGINS=http://localhost:3000,http://localhost:5173

# Telegram Bot Configuration (if needed)
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here
TELEGRAM_WEBHOOK_URL=

# Logging
LOG_LEVEL=INFO
